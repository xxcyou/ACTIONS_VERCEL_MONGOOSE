{
      "version": 2,
      "builds": [
            {
                  "src": "index.js",
                  "use": "@now/node",
                  "installCommand": "npm install",
                  "buildCommand": "npm run build"
            }
      ],
      "routes": [
            {
                  "src": "/(.*)",
                  "dest": "index.js"
            }
      ],
      "env": {
            "MONGO_URI": "${{ secrets.MONGO_URI }}"
      }
}